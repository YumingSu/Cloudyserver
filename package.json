{
  "name": "webapi",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "publish-coverage": "nyc report --reporter=text-lcov | coveralls",
    "coverage": "cross-env NODE_ENV=test PORT=4000 nyc mocha test/routes/products-test.js",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register test/routes/imitations-test.js",
    "postbuild": "npm run server",
    "prebuild": "npm-run-all test clean",
    "build": "babel ./ --out-dir lib/ --ignore ./node_modules,./.babelrc,./package.json,./package-lock.json,./test --copy-files",
    "server": "cross-env NODE_ENV=dev PORT=3001 node lib/bin/www",
    "start": "npm-run-all test build server",
    "clean": "rimraf ./lib && mkdir lib",
    "server:watch": "cross-env NODE_ENV=dev nodemon --exec babel-node bin/www",
    "test:watch": "npm run test -- --watch",
    "lint": "esw  ./test ./routes ./models/ app.js",
    "lint:watch": "npm run lint -- --watch"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "mongo": "^0.1.0",
    "mongodb": "^3.1.7",
    "mongoose": "^5.3.2",
    "morgan": "~1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "chai-things": "^0.2.0",
    "clean-webpack-plugin": "^0.1.19",
    "coveralls": "^2.13.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.18.8",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.22.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^0.5.8",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^2.9.1"
  }
}
